
{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
   
    "rootDir": "./",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "types": ["node"],
    "sourceMap": true,
    
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules","dist"]
}
